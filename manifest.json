{
  "name": "ShowYouTubeThumbnail",
  "version": "0.1",
  "manifest_version": 2,
  "description": "This is an extensions to show the thumbnail of the movie you are watching right now",
  "icons": {
    "24": "icon-24.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "ShowYouTubeTmb",
    "default_popup": "popup.html"
  }
}